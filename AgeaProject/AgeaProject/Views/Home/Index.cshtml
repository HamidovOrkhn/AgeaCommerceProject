            Swal.fire(
                'Successful Process!',
                $('#Success-Quote').val(),
                'success'
            )
            localStorage.clear();
        </script>
    }
    @if (TempData["Fail-Search"] is object)
    {
        <input type="hidden" id="Fail-Search" value="@TempData["Fail-Search"]" />
        <script>
            Swal.fire(
                'Could not Find',
                $('#Fail-Search').val(),
                'error'
            )
            localStorage.clear();
        </script>
    }
